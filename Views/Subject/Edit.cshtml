@model v_try.Models.Subject

@{
    ViewData["Title"] = "Edit Subject";
}

<h1>Edit Subject</h1>

<form asp-action="Edit" method="post">
    @Html.AntiForgeryToken()

    <input type="hidden" asp-for="Id" />

    <div class="form-group">
        <label asp-for="SubjectName">Subject Name</label>
        <input type="text" class="form-control" asp-for="SubjectName" required />
        <span asp-validation-for="SubjectName" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-success">Save Changes</button>
</form>


